import java.util.List;
import java.util.StringTokenizer;

public class Operaciones {

	public static void main(String[] args) {
		new Operaciones();
	}

	public Operaciones() {

	}

// 3
	public static int findMaxNumber(int arr[]) {
		int max = arr[0];
		for (int i = 1; i < arr.length; i++) {
			if (max < arr[i])
				max = arr[i];
		}
		return max;
	}
